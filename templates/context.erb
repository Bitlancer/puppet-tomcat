<Context path="<%= @path %>" docBase="<%= @docbase %>" debug="<%= @debug %>" useHttpOnly="<%= @httponly %>">
  <% resources.each do |resource| %>
  <Resource name="<% resource[name] %>" type="<% resource[type] %>"
    description="<% resource[description] %>" auth="<% resource[auth] %>" 
    factory="<% resource[factory] %>" jotm.timeout="<% resource[jotm.timeout] %>" />
  <% end %>
</Context>
